{
    "name": "ascend-sales-engine",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "AI-powered sales enablement platform with WhatsApp integration and meeting intelligence",
    "author": "Hunfly",
    "license": "UNLICENSED",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "format": "prettier --write \"app/**/*.{ts,tsx,css}\" \"src/**/*.{ts,tsx,css}\" \"*.{ts,json,mjs}\"",
        "format:check": "prettier --check \"app/**/*.{ts,tsx,css}\" \"src/**/*.{ts,tsx,css}\" \"*.{ts,json,mjs}\"",
        "type-check": "tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:e2e": "playwright test",
        "db:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
        "db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
        "db:studio": "drizzle-kit studio --config=drizzle.config.ts"
    },
    "dependencies": {
        "@fontsource/inter": "^5.2.8",
        "@fontsource/sora": "^5.2.8",
        "@radix-ui/react-accordion": "^1.1.2",
