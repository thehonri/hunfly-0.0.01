        "@types/pg": "^8.10.9",
        "@types/react": "^18.2.46",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "@vitest/ui": "^4.0.18",
        "autoprefixer": "^10.4.16",
        "drizzle-kit": "^0.31.8",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jsdom": "^23.0.0",
        "postcss": "^8.4.32",
        "prettier": "^3.1.1",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.3.3",
        "vitest": "^4.0.18"
    },
    "overrides": {
        "esbuild": "^0.25.1"
    }
